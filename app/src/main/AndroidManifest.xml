<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.coderamankumarguptaquizearn">
    <!-- When your device support above android version 8 then use "android:usesCleartextTraffic="true"" -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.QuizEarn"
        android:usesCleartextTraffic="true"
        tools:targetApi="31">
        <activity
            android:name="com.example.coderamankumarguptaquizearn.termCondition"
            android:exported="false" />
        <activity
            android:name="com.example.coderamankumarguptaquizearn.privacy"
            android:exported="false" />
        <activity
            android:name="com.example.coderamankumarguptaquizearn.SpinnerActivity"
            android:exported="false" />
        <activity
            android:name="com.example.coderamankumarguptaquizearn.ResultActivity"
            android:exported="true" />
        <activity
            android:name="com.example.coderamankumarguptaquizearn.MainActivity"
            android:exported="true" />
        <activity
            android:name="com.example.coderamankumarguptaquizearn.SignupActivity"
            android:exported="true" />
        <activity
            android:name="com.example.coderamankumarguptaquizearn.QuizActivity"
            android:exported="true" />
        <activity
            android:name="com.example.coderamankumarguptaquizearn.LoginActivity"
            android:exported="true">

            <!-- First Time Show Activity -->
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>